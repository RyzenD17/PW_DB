<Page x:Class="PW10_DB.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PW10_DB"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminPage">

    <Grid>
        <StackPanel>
            <Button Name="ShowData" Width="250" Height="50" Click="ShowData_Click" >Просмотр данных пользователей</Button>
            <Button Name="ShowOrders" Width="250" Height="50" Click="ShowOrders_Click">Просмотр заказов</Button>
            <Button Name="PersonalParlor" Width="250" Height="50" Click="PersonalParlor_Click" >Личный кабинет</Button>
            <Button Width="250" Height="50" Click="Button_Click">К авторизации</Button>
        </StackPanel>
        
    </Grid>
</Page>
